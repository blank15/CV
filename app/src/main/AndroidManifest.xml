<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gmail.yudistirosaputro.cv">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_cv"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_cv"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name=".SplashScreen"
            android:noHistory="true"
            android:theme="@style/AppTheme.NoActionBar">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>
        <activity
            android:name=".Pendidikan"
            android:label="@string/title_activity_pendidikan"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Karya"
            android:label="@string/title_activity_karya"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Gallery" />
        <activity
            android:name=".FullImage"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>